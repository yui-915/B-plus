token Comma=',' Semi=';' LPar='(' RPar=')' LBrace='{' RBrace='}'
      Extern='extern'
      Ident='</[_a-zA-Z][_a-zA-Z0-9]*/>' Integer='<[0-9]+>'
      Whitespace='</[ \\r\\n\\t]+/>'
;

skip Whitespace;
start file;

file: (
  function
| extern
)*;

function: Ident '(' ')' statement;

extern: 'extern' Ident ';';

statement: (
  function_call
|  block
) ';';

block: '{' statement* '}';
  
function_call: Ident([rvalue (',' rvalue)]);

rvalue: constant; 

constant: Integer;
